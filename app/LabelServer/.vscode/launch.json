    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../t2-output/linux-clang-debug-default/LabelServer",
            "args": [
                "/home/ben/mldata/gopro",
                "/home/ben/dev/Annotator/dims/roads-m3-cou.json",
                //"--export",
                //"/home/ben/mldata/train",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/../../t2-output/linux-clang-debug-default"
                }
            ],
            "preLaunchTask": "Build LabelServer",
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}